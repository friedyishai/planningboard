<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.whiteboard.controller.NewBoardController">
   <children>
      <Label layoutX="139.0" layoutY="48.0" text="Create New Board">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Label layoutX="76.0" layoutY="170.0" prefHeight="27.0" prefWidth="113.0" text="Board Name:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="boardNameField" layoutX="190.0" layoutY="163.0" prefHeight="41.0" prefWidth="222.0" promptText="board name" />
      <Button fx:id="createNewBoardBtn" layoutX="190.0" layoutY="251.0" mnemonicParsing="false" onAction="#createNewBoard" prefHeight="41.0" prefWidth="222.0" text="Create" />
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="back" />
   </children>
</AnchorPane>
