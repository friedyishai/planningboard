<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.whiteboard.controller.UserController">
   <children>
      <Label layoutX="258.0" layoutY="55.0" text="Login">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="225.0" layoutY="165.0" prefHeight="41.0" prefWidth="203.0" promptText="user name" />
      <TextField fx:id="passwordField" layoutX="225.0" layoutY="226.0" prefHeight="41.0" prefWidth="203.0" promptText="password" />
      <Label layoutX="120.0" layoutY="172.0" text="User Name:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="133.0" layoutY="226.0" text="Password:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="loginBtn" layoutX="225.0" layoutY="293.0" mnemonicParsing="false" onAction="#login" prefHeight="41.0" prefWidth="203.0" text="login" />
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="back" />
   </children>
</AnchorPane>
