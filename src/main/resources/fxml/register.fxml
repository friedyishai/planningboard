<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.whiteboard.controller.UserController">
    <Label layoutX="235.0" layoutY="30.0" text="Register">
        <font>
            <Font size="34.0" />
        </font>
    </Label>
    <TextField fx:id="emailField" layoutX="235.0" layoutY="118.0" prefHeight="41.0" prefWidth="203.0" promptText="email" />
    <TextField fx:id="usernameField" layoutX="235.0" layoutY="179.0" prefHeight="41.0" prefWidth="203.0" promptText="user name" />
    <Label layoutX="129.0" layoutY="186.0" text="User Name:">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Label layoutX="144.0" layoutY="243.0" text="Password:">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <Button fx:id="registerBtn" layoutX="235.0" layoutY="303.0" mnemonicParsing="false" onAction="#register" prefHeight="41.0" prefWidth="203.0" text="Register" />
    <Label layoutX="176.0" layoutY="125.0" prefHeight="27.0" prefWidth="48.0" text="Email:">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
   <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="back" />
   <PasswordField fx:id="passwordField" layoutX="235.0" layoutY="236.0" prefHeight="41.0" prefWidth="203.0" promptText="password" />
</AnchorPane>
